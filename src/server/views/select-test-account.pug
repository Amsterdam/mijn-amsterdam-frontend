h1 #{title}

style.
  h1 {
    text-align: center;
  }
  table {
    border-collapse: collapse;
    margin: 0 auto;
  }
  th {
    text-align: left;
    background-color: #f2f2f2;
    padding: 8px;
    text-align: left;
  }
  tr {
    background: #fff;
  }
  tr:nth-child(even) {
    background: #f2f2f2;
  }
  td {
    padding: 8px;
  }
  .test-account-name {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 300px;
  }

table
  thead
    tr
      th Naam
      th #{idLabel}
      th Mokum
      th Digid
      th Omschrijving
  tbody
    each account in testAccounts
      tr
        td
          a(href=account.authLoginRoute)
            div(class="test-account-name") #{account.username}
        td #{account.profileId}
        td #{account.mokum}
        td #{account.hasDigid}
        td #{account.description}
