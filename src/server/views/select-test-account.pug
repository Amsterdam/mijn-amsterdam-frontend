h1 #{title}

style.
  h1 {
    text-align: center;
  }
  table {
    border-collapse: collapse;
    margin: 0 auto;
  }
  th {
    text-align: left;
    background-color: #f2f2f2;
    padding: 8px;
    text-align: left;
  }
  tr {
    background: #fff;
  }
  tr:nth-child(even) {
    background: #f2f2f2;
  }
  td {
    padding: 8px;
  }
  .test-account-name {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 300px;
  }

table
  thead
    tr
      each header in tableHeaders
        th #{header}
  tbody
    each row in tableRows
      tr
        each field in row
          td !{field}
