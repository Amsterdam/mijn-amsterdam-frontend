  "scripts": {
    "analyze": "source-map-explorer 'build/assets/*.js'",
    "mock-server": "mocks-server",
    "mock-server:serve": "mocks-server --no-plugins.inquirerCli.enabled",
    "serve-dev": "concurrently \"npm:bff-api:watch\" \"npm:start\" \"npm:mock-server:serve\"",
    "start": "cross-env MA_APP_MODE=development vite",
    "build": "cross-env MA_APP_MODE=production vite build",
    "serve": "vite preview",
    "test": "cross-env MA_APP_MODE=unittest vitest src/client",
    "bff-api:watch": "nodemon -e ts -w ./src -w ./.env.local -x npm run bff-api:serve-dev",
    "bff-api:test": "cross-env MA_APP_MODE=unittest vitest --environment=node src/server src/universal",
    "bff-api:serve-build": "node build-bff/server/app",
    "bff-api:serve-dev": "cross-env NODE_OPTS=--inspect ts-node --project ./tsconfig.bff.json ./src/bffserver.ts",
    "bff-api:build": "tsc --project ./tsconfig.bff.json",
    "lint": "eslint src --max-warnings 0 --quiet",
    "lint:fix": "eslint src --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie > 0",
    "not op_mini all"
  ],
