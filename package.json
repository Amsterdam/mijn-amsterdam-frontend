{
  "name": "mijn-amsterdam-frontend",
  "version": "2.26.4",
  "scripts": {
    "build": "react-scripts build && npm run bff-api:build",
    "serve-build": "concurrently \"npm:bff-api:serve-build\" \"node scripts/serveBuild\"",
    "serve-dev": "concurrently \"npm:bff-api:watch\" \"npm:start\"",
    "release:bug": "sh scripts/release.sh --bug",
    "release:major": "sh scripts/release.sh --major",
    "release:minor": "sh scripts/release.sh --minor",
    "start": "react-scripts start",
    "test": "react-scripts test src/client",
    "test-e2e": "REACT_APP_ENV=e2e; react-scripts build && npm run bff-api:build && concurrently \"npm:serve-build\" \"npx cypress run\" --kill-others --success first",
    "bff-api:watch": "nodemon -e ts -w ./src -x npm run bff-api:serve-dev",
    "bff-api:test": "react-scripts test src/server",
    "bff-api:serve-build": "node build-bff/server/app",
    "bff-api:serve-dev": "ts-node --project ./tsconfig.bff.json ./src/bffserver.ts",
    "bff-api:build": "tsc --project ./tsconfig.bff.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "@datapunt/matomo-tracker-js": "0.0.15",
    "@datapunt/react-maps": "^0.7.2",
    "@sentry/browser": "^5.17.0",
    "@sentry/node": "^5.17.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.3",
    "@types/js-cookie": "^2.2.4",
    "@types/leaflet": "^1.5.8",
    "@types/memory-cache": "^0.2.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^12.12.28",
    "@types/proj4": "^2.5.0",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-transition-group": "^4.2.3",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/sanitize-html": "^1.23.0",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uid-safe": "^2.1.2",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "focus-trap-react": "^6.0.0",
    "include-media": "^1.4.9",
    "js-cookie": "^2.2.1",
    "jw-paginate": "^1.0.4",
    "leaflet": "^1.6.0",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "proj4": "^2.6.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-spring": "^8.0.27",
    "recoil": "0.0.10",
    "sanitize-html": "^1.26.0",
    "scrape-it": "^5.2.3",
    "slugme": "^1.1.1",
    "throttle-debounce": "^2.1.0",
    "typescript": "^3.8.2",
    "uid-safe": "^2.1.5",
    "use-debounce": "^3.3.0",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "concurrently": "^5.1.0",
    "cookie-session": "^1.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "http-proxy-middleware": "^1.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}
