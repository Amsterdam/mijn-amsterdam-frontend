{
  "name": "mijn-amsterdam-frontend",
  "version": "2.7.5",
  "scripts": {
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build && npm run bff-api:build",
    "serve-build": "concurrently \"npm:bff-api:serve-build\" \"node scripts/serveBuild\"",
    "serve-dev": "concurrently \"npm:bff-api:watch\" \"npm:start\"",
    "release:bug": "sh scripts/release.sh --bug",
    "release:major": "sh scripts/release.sh --major",
    "release:minor": "sh scripts/release.sh --minor",
    "start": "react-scripts start",
    "test": "react-scripts test src/client",
    "test-e2e": "REACT_APP_ENV=e2e; react-scripts build && npm run bff-api:build && concurrently \"npm:serve-build\" \"npx cypress run\" --kill-others --success first",
    "bff-api:watch": "nodemon -e ts -w ./src -x npm run bff-api:serve-dev",
    "bff-api:test": "react-scripts test src/server",
    "bff-api:serve-build": "node build-bff/server/app",
    "bff-api:serve-dev": "ts-node --project ./tsconfig.bff.json ./src/bffserver.ts",
    "bff-api:build": "tsc --project ./tsconfig.bff.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "@amsterdam/arm-cluster": "^0.4.0",
    "@amsterdam/arm-core": "^0.4.0",
    "@amsterdam/asc-assets": "^0.25.0",
    "@amsterdam/asc-ui": "^0.25.0",
    "@amsterdam/react-maps": "^0.10.0",
    "@datapunt/matomo-tracker-js": "^0.2.1",
    "@sentry/browser": "^5.20.1",
    "@sentry/node": "^5.20.1",
    "@types/classnames": "^2.2.10",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.7",
    "@types/js-cookie": "^2.2.6",
    "@types/leaflet": "^1.5.17",
    "@types/memory-cache": "^0.2.1",
    "@types/node": "^14.0.27",
    "@types/proj4": "^2.5.0",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/react-transition-group": "^4.4.0",
    "@types/sanitize-html": "^1.23.3",
    "@types/styled-components": "^5.1.2",
    "@types/supercluster": "^5.0.2",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uid-safe": "^2.1.2",
    "@types/ws": "^7.2.6",
    "axios": "^0.20.0",
    "axios-mock-adapter": "^1.18.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "focus-trap-react": "^8.0.0",
    "include-media": "^1.4.9",
    "js-cookie": "^2.2.1",
    "jw-paginate": "^1.0.4",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "memory-cache": "^0.2.0",
    "proj4": "^2.6.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^3.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-spring": "^8.0.27",
    "recoil": "0.0.10",
    "sanitize-html": "^2.0.0",
    "scrape-it": "^5.2.3",
    "slugme": "^1.1.1",
    "styled-components": "^5.1.1",
    "supercluster": "^7.1.0",
    "throttle-debounce": "^2.2.1",
    "typescript": "^4.0.3",
    "uid-safe": "^2.1.5",
