{
  "name": "mijn-amsterdam-frontend",
  "version": "1.1.0",
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@datapunt/matomo-tracker-js": "0.0.14",
    "@sentry/browser": "^5.6.3",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^11.13.20",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/throttle-debounce": "^1.1.1",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "classnames": "^2.2.6",
    "focus-trap-react": "^6.0.0",
    "http-server": "^0.11.1",
    "include-media": "^1.4.9",
    "node-persist": "^3.0.5",
    "react": "^16.9.0",
    "react-app-polyfill": "^0.2.2",
    "react-circular-progressbar": "^2.0.2",
    "react-dom": "^16.9.0",
    "react-error-boundary": "^1.2.5",
    "react-hooks-testing-library": "^0.5.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^2.1.8",
    "slug": "^1.1.0",
    "throttle-debounce": "^2.1.0",
    "typescript": "^3.6.2",
    "use-debounce": "^1.1.1",
    "use-media": "^1.4.0",
    "use-react-router": "^1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "lint:staged": "lint-staged",
    "mock-api": "dyson mock-api 5000 &",
    "watch:mock-api": "nodemon --watch mock-api dyson mock-api/ 5000",
    "serve-build": "http-server -p 3000 ./build &",
    "integration-tests": "npm run mock-api && npm run serve-build && npx cypress run && killall node",
    "release:major": "sh scripts/release.sh --major",
    "release:minor": "sh scripts/release.sh --minor",
    "release:bug": "sh scripts/release.sh --bug"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/slug": "^0.9.1",
    "dyson": "^2.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "1.16.4",
    "react-test-renderer": "^16.9.0"
  }
}
