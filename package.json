{
  "name": "mijn-amsterdam-frontend",
  "version": "2.26.6",
  "scripts": {
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build && npm run bff-api:build",
    "serve-build": "concurrently \"npm:bff-api:serve-build\" \"node scripts/serveBuild\"",
    "serve-dev": "concurrently \"npm:bff-api:watch\" \"npm:start\"",
    "release:bug": "sh scripts/release.sh --bug",
    "release:major": "sh scripts/release.sh --major",
    "release:minor": "sh scripts/release.sh --minor",
    "start": "react-scripts start",
    "test": "react-scripts test src/client",
    "test-e2e": "REACT_APP_ENV=e2e; react-scripts build && npm run bff-api:build && concurrently \"npm:serve-build\" \"npx cypress run\" --kill-others --success first",
    "bff-api:watch": "nodemon -e ts -w ./src -x npm run bff-api:serve-dev",
    "bff-api:test": "react-scripts test src/server",
    "bff-api:serve-build": "node build-bff/server/app",
    "bff-api:serve-dev": "ts-node --project ./tsconfig.bff.json ./src/bffserver.ts",
    "bff-api:build": "tsc --project ./tsconfig.bff.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "@amsterdam/arm-cluster": "^0.4.0",
    "@amsterdam/arm-core": "^0.4.0",
    "@amsterdam/asc-assets": "^0.26.2",
    "@amsterdam/asc-ui": "^0.26.2",
    "@amsterdam/react-maps": "^0.11.0",
    "@datapunt/matomo-tracker-js": "^0.3.1",
    "@sentry/browser": "^5.29.0",
    "@sentry/node": "^5.29.0",
    "@types/classnames": "^2.2.11",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.9",
    "@types/flat-cache": "^2.0.0",
    "@types/js-cookie": "^2.2.6",
    "@types/leaflet": "^1.5.19",
    "@types/memory-cache": "^0.2.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.13",
    "@types/proj4": "^2.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^17.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/sanitize-html": "^1.27.0",
    "@types/styled-components": "^5.1.5",
    "@types/supercluster": "^5.0.2",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uid-safe": "^2.1.2",
    "axios": "^0.21.0",
    "axios-mock-adapter": "^1.19.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
