{
  "name": "mijn-amsterdam-frontend",
  "version": "1.13.4",
  "scripts": {
    "build": "env-cmd --fallback -f ./.env.${REACT_APP_ENV:=production} react-scripts build",
    "e2e:local": "REACT_APP_ENV=e2e npm run build && concurrently \"npm:serve-build\" \"npx cypress open\" --kill-others --success first",
    "serve-build": "concurrently  \"npm:mock-api\" \"node scripts/serveBuild\"",
    "eject": "react-scripts eject",
    "mock-api": "npx dyson mock-api/ 5000",
    "release:bug": "sh scripts/release.sh --bug",
    "release:major": "sh scripts/release.sh --major",
    "release:minor": "sh scripts/release.sh --minor",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "watch:mock-api": "nodemon --watch mock-api/ --exec npm run mock-api"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "@datapunt/matomo-tracker-js": "0.0.15",
    "@datapunt/react-maps": "^0.7.2",
    "@sentry/browser": "^5.12.4",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/js-cookie": "^2.2.4",
    "@types/leaflet": "^1.5.8",
    "@types/node": "^12.12.28",
    "@types/proj4": "^2.5.0",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-transition-group": "^4.2.3",
    "@types/throttle-debounce": "^2.1.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.9.0",
    "focus-trap-react": "^6.0.0",
    "include-media": "^1.4.9",
    "js-cookie": "^2.2.1",
    "jw-paginate": "^1.0.4",
    "leaflet": "^1.6.0",
    "proj4": "^2.6.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-spring": "^8.0.27",
    "slugme": "^1.1.1",
    "throttle-debounce": "^2.1.0",
    "typescript": "^3.8.2",
    "use-debounce": "^3.3.0",
    "use-media": "^1.4.0",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "axios-mock-adapter": "^1.17.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-session": "^1.4.0",
    "dyson": "^3.0.1",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "http-proxy-middleware": "^1.0.0",
    "http-server": "^0.12.1",
    "node-sass": "^4.13.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}
