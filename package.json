{
  "name": "mijn-amsterdam-frontend",
  "version": "1.7.2",
  "scripts": {
    "build": "react-scripts build",
    "e2e:ci": "concurrently \"npm:serve-build\" \"npm:mock-api\" \"npx cypress run\" --kill-others --success first",
    "eject": "react-scripts eject",
    "lint:staged": "lint-staged",
    "mock-api": "npx dyson mock-api/ 5000",
    "release:bug": "sh scripts/release.sh --bug",
    "release:major": "sh scripts/release.sh --major",
    "release:minor": "sh scripts/release.sh --minor",
    "serve-build": "node scripts/proxy-serve",
    "serve-dev": "concurrently \"npm:serve-build\" \"npm:mock-api\"",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "watch:mock-api": "nodemon --watch mock-api/ --exec npm run mock-api"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "@datapunt/matomo-tracker-js": "0.0.15",
    "@datapunt/react-maps": "^0.7.1",
    "@sentry/browser": "^5.10.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/js-cookie": "^2.2.4",
    "@types/leaflet": "^1.5.8",
    "@types/node": "^12.12.14",
    "@types/proj4": "^2.5.0",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.1",
    "@types/react-transition-group": "^4.2.3",
    "@types/slug": "^0.9.1",
    "@types/throttle-debounce": "^2.1.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.8.1",
    "focus-trap-react": "^6.0.0",
    "http-proxy": "^1.18.0",
    "include-media": "^1.4.9",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.6.0",
    "mime-types": "^2.1.25",
    "proj4": "^2.6.0",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "react-spring": "^8.0.27",
    "slug": "^1.1.0",
    "throttle-debounce": "^2.1.0",
    "typescript": "^3.7.3",
    "use-debounce": "^3.3.0",
    "use-media": "^1.4.0",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "axios-mock-adapter": "^1.17.0",
    "concurrently": "^5.0.0",
    "dyson": "^3.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "http-server": "^0.12.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}
